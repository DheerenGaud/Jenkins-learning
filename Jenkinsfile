pipeline {
    agent any
    stages {
        stage("Clone") {
            steps {
                echo "clone the project"
            }
        }
        stage("Build") {
            steps {
                echo "Code Build Stage"
            }
        }
        stage("Test") {
            steps {
                echo "test started"
            }
        }
        stage("Deploy") {
            steps {
                echo "Deployed done"
            }
        }
    }
}
