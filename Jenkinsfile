                echo "clone the poject"
            }
        }
        stage("Build"){
            step{
                echo "Code Build Stage"
            }
        }
        stage("Test"){
           step{
                echo "test started"
            }
        }
        stage("Deploy"){
           step{
                echo "Deployed done"
            }
        }
    }
}